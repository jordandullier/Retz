<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.hackaton.retz" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Forêt de Retz</name>
    <description>
        Application mobile pour le centenaire de la première Guerre Mondiale
    </description>
    <author email="dev@callback.apache.org" href="http://cordova.io">
        Jordan DULLIER & William JEHANNE
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="AutoHideSplashScreen " value="false" />
    <preference name="splashscreen" value="ic_launcher" />
    <preference name="splashScreenDelay" value="7000" />
</widget>
